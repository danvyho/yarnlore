<% provide(:navbar_title, "Notifications") %>
<div class="container p-4">
  <div class="row">
    <div class="col-12">
      <div id="notifications" class="mt-4">
        <% @notifications.each do |notification|%>
          <%= render "notification", notification: notification %>
        <% end %>
      </div>
    </div>
  </div>
</div>
